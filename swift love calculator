import UIKit

func loveCalculator (yourName : String, thierName : String) -> String {
    
    let loveScore = arc4random_uniform(101)
    
    if loveScore > 80 {
        return "Your love score is \(loveScore). You love each other like Kanye loves Kanye"
    }
    else if loveScore > 40 && loveScore <= 80 {
        return "Your love score is \(loveScore). You go together like pizza and bananans"
    }
    
    else {
        return "Your love score is \(loveScore). You are Doomed to be forever alone!"
    }
    
}

print(loveCalculator (yourName: "Melissa Vento", thierName: "Danny Wolverton"))
